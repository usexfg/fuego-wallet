name: XFâ‚² Wallet Desktop Build & Release

on:
  push:
    branches: [main, master]
    paths:
      - "lib/**"
      - "android/**"
      - "ios/**"
      - "macos/**"
      - "windows/**"
      - "linux/**"
      - "pubspec.yaml"
      - ".github/workflows/flutter-desktop.yml"
  pull_request:
    branches: [main, master]
    paths:
      - "lib/**"
      - "android/**"
      - "ios/**"
      - "macos/**"
      - "windows/**"
      - "linux/**"
      - "pubspec.yaml"
      - ".github/workflows/flutter-desktop.yml"
